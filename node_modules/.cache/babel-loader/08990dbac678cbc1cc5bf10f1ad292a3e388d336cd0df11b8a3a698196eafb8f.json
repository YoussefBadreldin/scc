{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      universityNumber: '',\n      committeeInfo: null,\n      errorMessage: '',\n      committees: []\n    };\n  },\n  methods: {\n    async findCommittee() {\n      this.errorMessage = '';\n      const validUniversityNumbers = [\"20100294\", \"20100295\", \"20100296\", \"20100297\", \"20100298\"];\n      if (!validUniversityNumbers.includes(this.universityNumber)) {\n        this.errorMessage = 'الرقم الذي أدخلته غير صحيح، يرجى التحقق منه';\n        return;\n      }\n      const studentData = await this.fetchStudents();\n      const exampleData = studentData[this.universityNumber];\n      this.committeeInfo = exampleData;\n      if (this.committeeInfo) {\n        this.committees = await this.fetchCommitteesByLevel(exampleData.level);\n      }\n    },\n    async fetchStudents() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/students');\n        return await response.json();\n      } catch (error) {\n        console.error('Error fetching students:', error);\n        return {};\n      }\n    },\n    async fetchCommitteesByLevel(level) {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/clubs');\n        const clubsData = await response.json();\n        const responseSports = await fetch('https://aiusu-backend.vercel.app/sports');\n        const sportsData = await responseSports.json();\n        const responseCultural = await fetch('https://aiusu-backend.vercel.app/cultural');\n        const culturalData = await responseCultural.json();\n        const responseSocial = await fetch('https://aiusu-backend.vercel.app/social');\n        const socialData = await responseSocial.json();\n        const responseScout = await fetch('https://aiusu-backend.vercel.app/scout');\n        const scoutData = await responseScout.json();\n        const responseScientific = await fetch('https://aiusu-backend.vercel.app/scientific');\n        const scientificData = await responseScientific.json();\n        const responseArts = await fetch('https://aiusu-backend.vercel.app/arts');\n        const artsData = await responseArts.json();\n\n        // Combine all committees based on the level\n        const combinedCommittees = {\n          ...clubsData,\n          ...sportsData,\n          ...culturalData,\n          ...socialData,\n          ...scoutData,\n          ...scientificData,\n          ...artsData\n        };\n        return combinedCommittees[level] || [];\n      } catch (error) {\n        console.error('Error fetching committees:', error);\n        return [];\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","universityNumber","committeeInfo","errorMessage","committees","methods","findCommittee","validUniversityNumbers","includes","studentData","fetchStudents","exampleData","fetchCommitteesByLevel","level","response","fetch","json","error","console","clubsData","responseSports","sportsData","responseCultural","culturalData","responseSocial","socialData","responseScout","scoutData","responseScientific","scientificData","responseArts","artsData","combinedCommittees"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\elections.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <HeaderComponent />\r\n    <div class=\"container\">\r\n      <h1>Find Your Committee</h1>\r\n      <input v-model=\"universityNumber\" placeholder=\"Enter University Number\" />\r\n      <button @click=\"findCommittee\">Find Committee</button>\r\n      <p v-if=\"errorMessage\">{{ errorMessage }}</p>\r\n\r\n      <div v-if=\"committeeInfo\">\r\n        <h2>Committee Info</h2>\r\n        <p><strong>Name:</strong> {{ committeeInfo.name }}</p>\r\n        <p><strong>Faculty:</strong> {{ committeeInfo.faculty }}</p>\r\n        <p><strong>Level:</strong> {{ committeeInfo.level }}</p>\r\n        <p><strong>Location:</strong> {{ committeeInfo.committeeLocation }}</p>\r\n        <p><strong>List Number:</strong> {{ committeeInfo.listNumber }}</p>\r\n        <h3>Committees:</h3>\r\n        <ul>\r\n          <li v-for=\"committee in committees\" :key=\"committee.id\">\r\n            <img :src=\"committee.photo\" alt=\"Candidate Photo\" />\r\n            <strong>{{ committee.name }}</strong> - {{ committee.faculty }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <FooterComponent />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\n\r\nexport default {\r\n  name: 'FindCommittee',\r\n  components: {\r\n    HeaderComponent,\r\n    FooterComponent,\r\n  },\r\n  data() {\r\n    return {\r\n      universityNumber: '',\r\n      committeeInfo: null,\r\n      errorMessage: '',\r\n      committees: []\r\n    };\r\n  },\r\n  methods: {\r\n    async findCommittee() {\r\n      this.errorMessage = '';\r\n\r\n      const validUniversityNumbers = [\"20100294\", \"20100295\", \"20100296\", \"20100297\", \"20100298\"];\r\n\r\n      if (!validUniversityNumbers.includes(this.universityNumber)) {\r\n        this.errorMessage = 'الرقم الذي أدخلته غير صحيح، يرجى التحقق منه';\r\n        return;\r\n      }\r\n\r\n      const studentData = await this.fetchStudents();\r\n      const exampleData = studentData[this.universityNumber];\r\n\r\n      this.committeeInfo = exampleData;\r\n      if (this.committeeInfo) {\r\n        this.committees = await this.fetchCommitteesByLevel(exampleData.level);\r\n      }\r\n    },\r\n\r\n    async fetchStudents() {\r\n      try {\r\n        const response = await fetch('https://aiusu-backend.vercel.app/students');\r\n        return await response.json();\r\n      } catch (error) {\r\n        console.error('Error fetching students:', error);\r\n        return {};\r\n      }\r\n    },\r\n\r\n    async fetchCommitteesByLevel(level) {\r\n      try {\r\n        const response = await fetch('https://aiusu-backend.vercel.app/clubs');\r\n        const clubsData = await response.json();\r\n\r\n        const responseSports = await fetch('https://aiusu-backend.vercel.app/sports');\r\n        const sportsData = await responseSports.json();\r\n\r\n        const responseCultural = await fetch('https://aiusu-backend.vercel.app/cultural');\r\n        const culturalData = await responseCultural.json();\r\n\r\n        const responseSocial = await fetch('https://aiusu-backend.vercel.app/social');\r\n        const socialData = await responseSocial.json();\r\n\r\n        const responseScout = await fetch('https://aiusu-backend.vercel.app/scout');\r\n        const scoutData = await responseScout.json();\r\n\r\n        const responseScientific = await fetch('https://aiusu-backend.vercel.app/scientific');\r\n        const scientificData = await responseScientific.json();\r\n\r\n        const responseArts = await fetch('https://aiusu-backend.vercel.app/arts');\r\n        const artsData = await responseArts.json();\r\n\r\n        // Combine all committees based on the level\r\n        const combinedCommittees = {\r\n          ...clubsData,\r\n          ...sportsData,\r\n          ...culturalData,\r\n          ...socialData,\r\n          ...scoutData,\r\n          ...scientificData,\r\n          ...artsData,\r\n        };\r\n\r\n        return combinedCommittees[level] || [];\r\n      } catch (error) {\r\n        console.error('Error fetching committees:', error);\r\n        return [];\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n}\r\n.committee-container {\r\n    border: 1px solid #ddd;\r\n    padding: 15px;\r\n    border-radius: 8px;\r\n}\r\n.committee-container h6 {\r\n    margin-bottom: 10px;\r\n    font-size: 1.5em; /* Larger font size for committee name */\r\n    font-weight: bold; /* Bold the committee name */\r\n}\r\n.committee-container ul {\r\n    list-style: none;\r\n    padding-left: 0;\r\n    display: flex; /* Use flexbox for the list */\r\n    flex-wrap: wrap; /* Allow wrapping to the next line if needed */\r\n    justify-content: space-between; /* Space out items evenly */\r\n}\r\n.candidate-item {\r\n    display: flex;\r\n    flex-direction: column; /* Stack the elements vertically */\r\n    align-items: center; /* Center align items */\r\n    margin: 15px; /* Space between candidates */\r\n    flex: 0 1 calc(33.333% - 30px); /* Allow a maximum of 3 items per row */\r\n    box-sizing: border-box; /* Ensure padding and margin are included in width */\r\n}\r\n.candidate-photo img {\r\n    width: 70px; /* Adjust width as needed */\r\n    height: auto; /* Maintain aspect ratio */\r\n    border-radius: 50%; /* Optional: make photo circular */\r\n}\r\n.candidate-name {\r\n    font-weight: bold; /* Bold text for name */\r\n    margin-top: 5px; /* Space between photo and name */\r\n    font-size: 1em; /* Default font size for candidate name */\r\n}\r\n.candidate-faculty {\r\n    color: #555; /* Optional: different color for faculty */\r\n    margin-top: 2px; /* Space between name and faculty */\r\n    display: block; /* Ensure faculty is on a new line */\r\n    font-size: 0.9em; /* Slightly smaller font size for faculty */\r\n}\r\n.alert {\r\n    margin-top: 20px; /* Margin for alert message */\r\n}\r\n.video-container {\r\n    margin-top: 20px; /* Margin for video container */\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,eAAe;IACfC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE,IAAI;MACnBC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC