{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      president: {},\n      vicePresident: {},\n      heads: {\n        Head_Scout: {},\n        Head_Scientific: {},\n        Head_Cultural: {},\n        Head_Clubs: {},\n        Head_Arts: {},\n        Head_Sports: {},\n        Head_Social: {}\n      },\n      viceHeads: {\n        Vice_Scout: {},\n        Vice_Scientific: {},\n        Vice_Cultural: {},\n        Vice_Clubs: {},\n        Vice_Arts: {},\n        Vice_Sports: {},\n        Vice_Social: {}\n      },\n      members: {\n        Member_Scout: [],\n        Member_Scientific: [],\n        Member_Cultural: [],\n        Member_Clubs: [],\n        Member_Arts: [],\n        Member_Sports: [],\n        Member_Social: []\n      },\n      loading: true,\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const memberData = await response.json();\n        this.organizeMembers(memberData);\n        this.loading = false;\n      } catch (error) {\n        this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\n        console.error('Error loading member data:', error);\n        this.loading = false;\n      }\n    },\n    organizeMembers(members) {\n      const titles = ['President', 'Vice', 'Head_Scout', 'Head_Scientific', 'Head_Cultural', 'Head_Clubs', 'Head_Arts', 'Head_Sports', 'Head_Social', 'Vice_Scout', 'Vice_Scientific', 'Vice_Cultural', 'Vice_Clubs', 'Vice_Arts', 'Vice_Sports', 'Vice_Social', 'Member_Scout', 'Member_Scientific', 'Member_Cultural', 'Member_Clubs', 'Member_Arts', 'Member_Sports', 'Member_Social'];\n\n      // Create a mapping for roles\n      const roleMap = {\n        'President': () => this.president,\n        'Vice': () => this.vicePresident,\n        'Head_Scout': () => this.heads.Head_Scout,\n        'Head_Scientific': () => this.heads.Head_Scientific,\n        'Head_Cultural': () => this.heads.Head_Cultural,\n        'Head_Clubs': () => this.heads.Head_Clubs,\n        'Head_Arts': () => this.heads.Head_Arts,\n        'Head_Sports': () => this.heads.Head_Sports,\n        'Head_Social': () => this.heads.Head_Social,\n        'Vice_Scout': () => this.viceHeads.Vice_Scout,\n        'Vice_Scientific': () => this.viceHeads.Vice_Scientific,\n        'Vice_Cultural': () => this.viceHeads.Vice_Cultural,\n        'Vice_Clubs': () => this.viceHeads.Vice_Clubs,\n        'Vice_Arts': () => this.viceHeads.Vice_Arts,\n        'Vice_Sports': () => this.viceHeads.Vice_Sports,\n        'Vice_Social': () => this.viceHeads.Vice_Social,\n        'Member_Scout': () => this.members.Member_Scout,\n        'Member_Scientific': () => this.members.Member_Scientific,\n        'Member_Cultural': () => this.members.Member_Cultural,\n        'Member_Clubs': () => this.members.Member_Clubs,\n        'Member_Arts': () => this.members.Member_Arts,\n        'Member_Sports': () => this.members.Member_Sports,\n        'Member_Social': () => this.members.Member_Social\n      };\n\n      // Organize members in a single loop\n      for (const member of members) {\n        const title = member.member_title;\n        if (title in roleMap) {\n          const role = roleMap[title];\n          if (Array.isArray(role())) {\n            role().push(member); // Push to array for members\n          } else {\n            Object.assign(role(), member); // Assign to object for roles\n          }\n        }\n      }\n    },\n    // New method to extract the first and last words from candidate names\n    formatMemberName(name) {\n      if (!name) return '';\n      const words = name.split(' ');\n      return words.length === 1 ? words[0] : `${words[0]} ${words[words.length - 1]}`; // Return first and last words\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","president","vicePresident","heads","Head_Scout","Head_Scientific","Head_Cultural","Head_Clubs","Head_Arts","Head_Sports","Head_Social","viceHeads","Vice_Scout","Vice_Scientific","Vice_Cultural","Vice_Clubs","Vice_Arts","Vice_Sports","Vice_Social","members","Member_Scout","Member_Scientific","Member_Cultural","Member_Clubs","Member_Arts","Member_Sports","Member_Social","loading","errorMessage","mounted","loadMemberData","methods","response","fetch","ok","Error","memberData","json","organizeMembers","error","console","titles","roleMap","President","Vice","member","title","member_title","role","Array","isArray","push","Object","assign","formatMemberName","words","split","length"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">أعضاء الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${president.member_id}.jpg`\" :alt=\"president.member_name\" class=\"circle-photo\" v-if=\"president.member_id\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ formatmemberName(president.member_name) || 'جاري التحميل..' }}</div>\r\n                        <div class=\"title\">رئيس الاتحاد</div>\r\n                        <div class=\"faculty\">{{ president.member_faculty || 'جاري التحميل..' }}</div>\r\n                        <div cl