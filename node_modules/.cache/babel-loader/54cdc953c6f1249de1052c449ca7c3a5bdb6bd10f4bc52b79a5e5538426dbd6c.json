{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'OrganizationalChart',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      organization: {\n        president: {\n          name: 'رئيس الاتحاد',\n          photo: '../../images/president.jpg',\n          faculty: 'هندسة',\n          level: 'سنة نهائية'\n        },\n        vicePresident: {\n          name: 'نائب رئيس الاتحاد',\n          photo: '../../images/vice-president.jpg',\n          faculty: 'إدارة أعمال',\n          level: 'سنة ثانية'\n        },\n        committees: [{\n          name: 'لجنة الأسر',\n          head: {\n            name: 'أمين لجنة الأسر',\n            photo: '../../images/head1.jpg',\n            faculty: 'حقوق',\n            level: 'سنة نهائية'\n          },\n          viceHead: {\n            name: 'أمين مساعد لجنة الأسر',\n            photo: '../../images/vice-head1.jpg',\n            faculty: 'حقوق',\n            level: 'سنة ثانية'\n          },\n          members: [{\n            name: 'عضو 1',\n            photo: '../../images/member1.jpg',\n            faculty: 'حقوق',\n            level: 'سنة أولى'\n          }\n          // أضف المزيد من الأعضاء...\n          ]\n        }, {\n          name: 'اللجنة الرياضية',\n          head: {\n            name: 'أمين اللجنة الرياضية',\n            photo: '../../images/head2.jpg',\n            faculty: 'طب',\n            level: 'سنة نهائية'\n          },\n          viceHead: {\n            name: 'أمين مساعد اللجنة الرياضية',\n            photo: '../../images/vice-head2.jpg',\n            faculty: 'طب',\n            level: 'سنة ثانية'\n          },\n          members: [{\n            name: 'عضو 1',\n            photo: '../../images/member9.jpg',\n            faculty: 'طب',\n            level: 'سنة أولى'\n          }\n          // أضف المزيد من الأعضاء...\n          ]\n        }\n        // تكرار لبقية اللجان...\n        ]\n      }\n    };\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","organization","president","photo","faculty","level","vicePresident","committees","head","viceHead","members"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n\r\n            <div class=\"org-chart\">\r\n                <div class=\"level president\">\r\n                    <div class=\"person text-center\">\r\n                        <img :src=\"organization.president.photo\" alt=\"صورة رئيس الاتحاد\" class=\"photo\" />\r\n                        <p class=\"name\">{{ organization.president.name }}</p>\r\n                        <p class=\"faculty\">{{ organization.president.faculty }}</p>\r\n                        <p class=\"level\">{{ organization.president.level }}</p>\r\n                        <p class=\"title\">رئيس الاتحاد</p>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"arrow\">↓</div>\r\n\r\n                <div class=\"level vice-president\">\r\n                    <div class=\"person text-center\">\r\n                        <img :src=\"organization.vicePresident.photo\" alt=\"صورة نائب رئيس الاتحاد\" class=\"photo\" />\r\n                        <p class=\"name\">{{ organization.vicePresident.name }}</p>\r\n                        <p class=\"faculty\">{{ organization.vicePresident.faculty }}</p>\r\n                        <p class=\"level\">{{ organization.vicePresident.level }}</p>\r\n                        <p class=\"title\">نائب رئيس الاتحاد</p>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"arrow\">↓</div>\r\n\r\n                <div class=\"committees\">\r\n     