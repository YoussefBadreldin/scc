{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nimport axios from 'axios';\nexport default {\n  name: 'JoinUnion',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      members: {},\n      clubs: [],\n      committees: []\n    };\n  },\n  mounted() {\n    this.fetchMembersData();\n    window.scrollTo(0, 0);\n  },\n  methods: {\n    async fetchMembersData() {\n      try {\n        const response = await axios.get('https://aiusu-backend.vercel.app/members');\n        const data = response.data;\n\n        // Assuming the response is an array of members\n        this.members = {};\n        data.forEach(member => {\n          // Using member_id as the key\n          this.members[member.member_id] = member;\n        });\n\n        // Example club and committee structure based on your requirements\n        this.clubs = [\n        // Populate club data based on your application logic\n        {\n          member_id: '3',\n          viceMembers: [{\n            member_id: '4'\n          }]\n        }];\n        this.committees = [{\n          name: 'لجنة الأسر',\n          head_id: '3',\n          members: [{\n            member_id: '5'\n          }, {\n            member_id: '6'\n          }]\n        }];\n      } catch (error) {\n        console.error('Error fetching member data:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","axios","name","components","data","members","clubs","committees","mounted","fetchMembersData","window","scrollTo","methods","response","get","forEach","member","member_id","viceMembers","head_id","error","console"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\" v-if=\"members\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"members.president.image\" alt=\"رئيس الاتحاد\" class=\"circle-photo\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ members.president.member_name }}</div>\r\n                        <div class=\"title\">{{ members.president.member_title }}</div>\r\n                        <div class=\"faculty\">{{ members.president.member_faculty }}</div>\r\n                        <div class=\"level\">المستوى {{ members.president.member_level }}</div>\r\n                    </div>\r\n                    <div class=\"subordinates\">\r\n                        <div class=\"position vice-president\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"members.vice.image\" alt=\"نائب رئيس الاتحاد\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ members.vice.member_name }}</div>\r\n                                <div class=\"title\">{{ members.vice.member_title }}</div>\r\n                                <div class=\"faculty\">{{ members.vice.member_faculty }}</div>\r\n                                <div class=\"level\">المستوى {{ members.vice.member_level }}</div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"subordinates\">\r\n                            <div class=\"position club-head\" v-for=\"club in clubs\" :key=\"club.member_id\">\r\n                                <div class=\"profile\">\r\n                                    <img :src=\"members[club.member_id].image\" alt=\"رئيس النادي\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\">\r\n                                    <div class=\"name\">{{ members[club.member_id].member_name }}</div>\r\n                                    <div class=\"title\">{{ members[club.member_id].member_title }}</div>\r\n                                    <div class=\"faculty\">{{ members[club.member_id].member_faculty }}</div>\r\n                                    <div class=\"level\">المستوى {{ members[club.member_id].member_level }}</div>\r\n                                </div>\r\n                                <div class=\"subordinates\">\r\n                                    <div class=\"position club-vice\" v-for=\"member in club.viceMembers\" :key=\"member.member_id\">\r\n                                        <div class=\"profile\">\r\n                                            <img :src=\"members[member.member_id].image\" alt=\"نائب رئيس النادي\" class=\"circle-photo\" />\r\n                                        </div>\r\n                                        <div class=\"details\">\r\n                                            <div class=\"name\">{{ members[member.member_id].member_name }}</div>\r\n                                            <div class=\"title\">{{ members[member.member_id].member_title }}</div>\r\n                                            <div class=\"faculty\">{{ members[member.member_id].member_faculty }}</div>\r\n                                            <div class=\"level\">المستوى {{ members[member.member_id].member_level }}</div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <h2 class=\"text-center my-4\">المكاتب التنفيذية للجان الأنشطة</h2>\r\n            <div class=\"executive-committee\">\r\n                <div class=\"committee-box\" v-for=\"(committee, index) in committees\" :key=\"index\">\r\n                    <div class=\"committee-title\">{{ committee.name }}</div>\r\n                    <div class=\"committee-content\">\r\n                        <div class=\"position head\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"members[committee.head_id].image\" alt=\"أمين اللجنة\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ members[committee.head_id].member_name }}</div>\r\n                                <div class=\"title\">{{ members[committee.head_id].member_title }}</div>\r\n                                <div class=\"faculty\">{{ members[committee.head_id].member_faculty }}</div>\r\n                                <div class=\"level\">المستوى {{ members[committee.head_id].member_level }}</div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"members\">\r\n                            <div class=\"member\" v-for=\"member in committee.members\" :key=\"member.member_id\">\r\n                                <div class=\"profile\">\r\n                                    <img :src=\"members[member.member_id].image\" alt=\"عضو باللجنة\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\">\r\n                                    <div class=\"name\">{{ members[member.member_id].member_name }}</div>\r\n                                    <div class=\"title\">{{ members[member.member_id].member_title }}</div>\r\n                                    <div class=\"faculty\">{{ members[member.faculty] }}</div>\r\n                                    <div class=\"level\">المستوى {{ members[member.member_id].member_level }}</div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'JoinUnion',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n    },\r\n    data() {\r\n        return {\r\n            members: {},\r\n            clubs: [],\r\n            committees: [],\r\n        };\r\n    },\r\n    mounted() {\r\n        this.fetchMembersData();\r\n        window.scrollTo(0, 0);\r\n    },\r\n    methods: {\r\n        async fetchMembersData() {\r\n            try {\r\n                const response = await axios.get('https://aiusu-backend.vercel.app/members');\r\n                const data = response.data;\r\n\r\n                // Assuming the response is an array of members\r\n                this.members = {};\r\n                data.forEach(member => {\r\n                    // Using member_id as the key\r\n                    this.members[member.member_id] = member;\r\n                });\r\n\r\n                // Example club and committee structure based on your requirements\r\n                this.clubs = [\r\n                    // Populate club data based on your application logic\r\n                    { member_id: '3', viceMembers: [{ member_id: '4' }] },\r\n                ];\r\n                this.committees = [\r\n                    {\r\n                        name: 'لجنة الأسر',\r\n                        head_id: '3',\r\n                        members: [\r\n                            { member_id: '5' },\r\n                            { member_id: '6' },\r\n                        ],\r\n                    },\r\n                ];\r\n            } catch (error) {\r\n                console.error('Error fetching member data:', error);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing styles remain unchanged */\r\n.container {\r\n    padding: 20px;\r\n    direction: rtl; /* Set the text direction to RTL */\r\n}\r\n.organization-chart {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin: 20px 0;\r\n}\r\n.position {\r\n    margin: 10px 0;\r\n    text-align: center;\r\n}\r\n.profile {\r\n    margin: 10px;\r\n}\r\n.circle-photo {\r\n    border-radius: 50%;\r\n    width: 80px; /* Adjust size as necessary */\r\n    height: 80px; /* Ensure it’s a circle */\r\n}\r\n.details {\r\n    padding: 5px;\r\n}\r\n.name {\r\n    font-weight: bold;\r\n}\r\n.title {\r\n    font-weight: bold;\r\n}\r\n.subordinates {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.executive-committee {\r\n    display: flex;\r\n    flex-wrap: wrap; /* Allow wrapping to create multiple rows */\r\n    justify-content: space-between; /* Distribute space between committee boxes */\r\n    gap: 10px; /* Optional: Add some spacing between boxes */\r\n}\r\n.committee-box {\r\n    border: 1px solid #dee2e6;\r\n    border-radius: 5px;\r\n    padding: 10px;\r\n    width: calc(33.33% - 20px); /* 3 boxes per row minus margin */\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    box-sizing: border-box; /* Include padding and border in element's total width and height */\r\n}\r\n.committee-title {\r\n    font-weight: bold;\r\n    font-size: 1.2rem;\r\n    text-align: center;\r\n    margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAgGA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AACrE,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACRJ,eAAe;IACfC;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,CAAC,CAAC;MACXC,KAAK,EAAE,EAAE;MACTC,UAAU