{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      president: {},\n      vicePresident: {},\n      heads: {},\n      viceHeads: {},\n      members: {},\n      loading: true,\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const members = await response.json();\n\n        // Organize members\n        this.organizeMembers(members);\n        this.loading = false;\n      } catch (error) {\n        this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\n        console.error('Error loading member data:', error);\n        this.loading = false;\n      }\n    },\n    organizeMembers(members) {\n      // Extract president and vice president\n      this.president = members.find(m => m.member_title === 'President');\n      this.vicePresident = members.find(m => m.member_title === 'Vice');\n\n      // Organize heads and vice heads\n      const titles = ['Head_Scout', 'Head_Scientific', 'Head_Cultural', 'Head_Clubs', 'Head_Arts', 'Head_Sports', 'Head_Social'];\n      const viceTitles = ['Vice_Scout', 'Vice_Scientific', 'Vice_Cultural', 'Vice_Clubs', 'Vice_Arts', 'Vice_Sports', 'Vice_Social'];\n      titles.forEach(title => {\n        this.heads[title] = members.find(m => m.member_title === title);\n      });\n      viceTitles.forEach(title => {\n        this.viceHeads[title] = members.find(m => m.member_title === title);\n      });\n\n      // Organize members by title\n      this.members['Member_Scout'] = members.filter(m => m.member_title === 'Member_Scout');\n      this.members['Member_Scientific'] = members.filter(m => m.member_title === 'Member_Scientific');\n      this.members['Member_Cultural'] = members.filter(m => m.member_title === 'Member_Cultural');\n      this.members['Member_Clubs'] = members.filter(m => m.member_title === 'Member_Clubs');\n      // Add more categories as needed\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","president","vicePresident","heads","viceHeads","members","loading","errorMessage","mounted","loadMemberData","methods","response","fetch","ok","Error","json","organizeMembers","error","console","find","m","member_title","titles","viceTitles","forEach","title","filter"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${president.member_id}.jpg`\" :alt=\"president.member_name\" class=\"circle-photo\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ president.member_name }}</div>\r\n                        <div class=\"title\">رئيس الاتحاد</div>\r\n                        <div class=\"faculty\">{{ president.member_faculty }}</div>\r\n                        <div class=\"level\">{{ president.member_level }}</div>\r\n                    </div>\r\n                    <div class=\"subordinates\">\r\n                        <div class=\"position vice-president\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${vicePresident.member_id}.jpg`\" :alt=\"vicePresident.member_name\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ vicePresident.member_name }}</div>\r\n                                <div class=\"title\">نائب رئيس الاتحاد</div>\r\n                                <div class=\"faculty\">{{ vicePresident.member_faculty }}</div>\r\n                                <div class=\"level\">{{ vicePresident.member_level }}</div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <h2 class=\"text-center my-4\">المكاتب التنفيذية للجان الأنشطة</h2>\r\n            <div class=\"executive-committee\">\r\n                <!-- Committee for Scouts -->\r\n                <div class=\"committee-box\">\r\n                    <div class=\"committee-title\">لجنة الجوالة والخدمة العامة</div>\r\n                    <div class=\"committee-content\">\r\n                        <div class=\"position head\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${heads['Head_Scout'].member_id}.jpg`\" :alt=\"heads['Head_Scout'].member_name\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ heads['Head_Scout'].member_name }}</div>\r\n                                <div class=\"title\">أمين لجنة الجوالة والخدمة العامة</div>\r\n                                <div class=\"faculty\">{{ heads['Head_Scout'].member_faculty }}</div>\r\n                                <div class=\"level\">{{ heads['Head_Scout'].member_level }}</div>\r\n                            </div>\r\n                            <div class=\"subordinates\">\r\n                                <div class=\"position vice-head\">\r\n                                    <div class=\"profile\">\r\n                                        <img :src=\"`../../images/members/${viceHeads['Vice_Scout'].member_id}.jpg`\" :alt=\"viceHeads['Vice_Scout'].member_name\" class=\"circle-photo\" />\r\n                                    </div>\r\n                                    <div class=\"details\">\r\n                                        <div class=\"name\">{{ viceHeads['Vice_Scout'].member_name }}</div>\r\n                                        <div class=\"title\">أمين مساعد لجنة الجوالة والخدمة العامة</div>\r\n                                        <div class=\"faculty\">{{ viceHeads['Vice_Scout'].member_faculty }}</div>\r\n                                        <div class=\"level\">{{ viceHeads['Vice_Scout'].member_level }}</div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"members\">\r\n                            <div class=\"member\" v-for=\"member in members['Member_Scout']\" :key=\"member.member_id\">\r\n                                <div class=\"profile\">\r\n                                    <img :src=\"`../../images/members/${member.member_id}.jpg`\" :alt=\"member.member_name\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\">\r\n                                    <div class=\"name\">{{ member.member_name }}</div>\r\n                                    <div class=\"title\">عضو بلجنة الجوالة والخدمة العامة</div>\r\n                                    <div class=\"faculty\">{{ member.member_faculty }}</div>\r\n                                    <div class=\"level\">{{ member.member_level }}</div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <!-- Repeat similar blocks for other committees -->\r\n                <div class=\"committee-box\">\r\n                    <div class=\"committee-title\">اللجنة العلمية</div>\r\n                    <div class=\"committee-content\">\r\n                        <div class=\"position head\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${heads['Head_Scientific'].member_id}.jpg`\" :alt=\"heads['Head_Scientific'].member_name\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ heads['Head_Scientific'].member_name }}</div>\r\n                                <div class=\"title\">أمين اللجنة العلمية</div>\r\n                                <div class=\"faculty\">{{ heads['Head_Scientific'].member_faculty }}</div>\r\n                                <div class=\"level\">{{ heads['Head_Scientific'].member_level }}</div>\r\n                            </div>\r\n                            <div class=\"subordinates\">\r\n                                <div class=\"position vice-head\">\r\n                                    <div class=\"profile\">\r\n                                        <img :src=\"`../../images/members/${viceHeads['Vice_Scientific'].member_id}.jpg`\" :alt=\"viceHeads['Vice_Scientific'].member_name\" class=\"circle-photo\" />\r\n                                    </div>\r\n                                    <div class=\"details\">\r\n                                        <div class=\"name\">{{ viceHeads['Vice_Scientific'].member_name }}</div>\r\n                                        <div class=\"title\">أمين مساعد اللجنة العلمية</div>\r\n                                        <div class=\"faculty\">{{ viceHeads['Vice_Scientific'].member_faculty }}</div>\r\n                                        <div class=\"level\">{{ viceHeads['Vice_Scientific'].member_level }}</div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"members\">\r\n                            <div class=\"member\" v-for=\"member in members['Member_Scientific']\" :key=\"member.member_id\">\r\n                                <div class=\"profile\">\r\n                                    <img :src=\"`../../images/members/${member.member_id}.jpg`\" :alt=\"member.member_name\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\">\r\n                                    <div class=\"name\">{{ member.member_name }}</div>\r\n                                    <div class=\"title\">عضو باللجنة العلمية</div>\r\n                                    <div class=\"faculty\">{{ member.member_faculty }}</div>\r\n                                    <div class=\"level\">{{ member.member_level }}</div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <!-- You can repeat for other committee titles like Head_Cultural, Head_Clubs, etc. -->\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\n\r\nexport default {\r\n    name: 'FindCommittee',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n    },\r\n    data() {\r\n        return {\r\n            president: {},\r\n            vicePresident: {},\r\n            heads: {},\r\n            viceHeads: {},\r\n            members: {},\r\n            loading: true,\r\n            errorMessage: '',\r\n        };\r\n    },\r\n    mounted() {\r\n        this.loadMemberData();\r\n    },\r\n    methods: {\r\n        async loadMemberData() {\r\n            try {\r\n                const response = await fetch('https://aiusu-backend.vercel.app/members');\r\n                if (!response.ok) throw new Error('Failed to fetch member data');\r\n                const members = await response.json();\r\n\r\n                // Organize members\r\n                this.organizeMembers(members);\r\n                this.loading = false;\r\n            } catch (error) {\r\n                this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\r\n                console.error('Error loading member data:', error);\r\n                this.loading = false;\r\n            }\r\n        },\r\n\r\n        organizeMembers(members) {\r\n            // Extract president and vice president\r\n            this.president = members.find(m => m.member_title === 'President');\r\n            this.vicePresident = members.find(m => m.member_title === 'Vice');\r\n\r\n            // Organize heads and vice heads\r\n            const titles = [\r\n                'Head_Scout', 'Head_Scientific', 'Head_Cultural', 'Head_Clubs',\r\n                'Head_Arts', 'Head_Sports', 'Head_Social'\r\n            ];\r\n            const viceTitles = [\r\n                'Vice_Scout', 'Vice_Scientific', 'Vice_Cultural', 'Vice_Clubs',\r\n                'Vice_Arts', 'Vice_Sports', 'Vice_Social'\r\n            ];\r\n\r\n            titles.forEach(title => {\r\n                this.heads[title] = members.find(m => m.member_title === title);\r\n            });\r\n\r\n            viceTitles.forEach(title => {\r\n                this.viceHeads[title] = members.find(m => m.member_title === title);\r\n            });\r\n\r\n            // Organize members by title\r\n            this.members['Member_Scout'] = members.filter(m => m.member_title === 'Member_Scout');\r\n            this.members['Member_Scientific'] = members.filter(m => m.member_title === 'Member_Scientific');\r\n            this.members['Member_Cultural'] = members.filter(m => m.member_title === 'Member_Cultural');\r\n            this.members['Member_Clubs'] = members.filter(m => m.member_title === 'Member_Clubs');\r\n            // Add more categories as needed\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n    direction: rtl; /* Set the text direction to RTL */\r\n}\r\n.organization-chart {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin: 20px 0;\r\n}\r\n.position {\r\n    margin: 10px 0;\r\n    text-align: center;\r\n}\r\n.profile {\r\n    margin: 10px;\r\n}\r\n.circle-photo {\r\n    border-radius: 50%;\r\n    width: 80px; /* Adjust size as necessary */\r\n    height: 80px; /* Ensure it’s a circle */\r\n}\r\n.details {\r\n    padding: 5px;\r\n}\r\n.name {\r\n    font-weight: bold;\r\n}\r\n.title {\r\n    font-weight: bold;\r\n}\r\n.subordinates {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.executive-committee {\r\n    display: flex;\r\n    flex-wrap: wrap; /* Allow wrapping to create multiple rows */\r\n    justify-content: space-between; /* Distribute space between committee boxes */\r\n    gap: 10px; /* Optional: Add some spacing between boxes */\r\n}\r\n.committee-box {\r\n    border: 1px solid #dee2e6;\r\n    border-radius: 5px;\r\n    padding: 10px;\r\n    width: calc(33.33% - 20px); /* 3 boxes per row minus margin */\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    box-sizing: border-box; /* Include padding and border in element's total width and height */\r\n}\r\n.committee-title {\r\n    font-weight: bold;\r\n    font-size: 1.2rem;\r\n    text-align: center;\r\n    margin-bottom: 10px;\r\n}\r\n.committee-content {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n.members {\r\n    display: flex;\r\n    flex-wrap: wrap; /* Allow members to wrap to the next line */\r\n    justify-content: space-around; /* Center members with space around */\r\n}\r\n\r\n.member {\r\n    width: 25%; /* 4 members per row */\r\n    margin-bottom: 10px; /* Add some space between rows */\r\n    box-sizing: border-box; /* Include padding and borders in width */\r\n    text-align: center; /* Center member details */\r\n}\r\n@media (max-width: 768px) {\r\n    .committee-box {\r\n        width: calc(50% - 20px); /* 2 boxes per row on tablets */\r\n    }\r\n}\r\n@media (max-width: 576px) {\r\n    .committee-box {\r\n        width: 100%; /* 1 box per row on mobile */\r\n    }\r\n    .title {\r\n        font-size: 0.8rem; /* Reduce font size on small screens */\r\n    }\r\n    .committee-box {\r\n        width: 100%; /* 1 box per row on mobile */\r\n    }\r\n    .title, .name, .committee-title {\r\n        font-size: 0.45rem; /* Reduce font size on small screens */\r\n    }\r\n    \r\n    h2 {\r\n        font-size: 1.8rem; /* Reduce heading size on mobile */\r\n    }\r\n    .details {\r\n        font-size: 0.45rem; /* Reduce overall details font size */\r\n    }\r\n}\r\n\r\n</style>\r\n"],"mappings":"AAkIA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,CAAC,CAAC;MACbC,aAAa,EAAE,CAAC,CAAC;MACjBC,KAAK,EAAE,CAAC,CAAC;MACTC,SAAS,EAAE,CAAC,CAAC;MACbC,OAAO,EAAE,CAAC,CAAC;MACXC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,cAAc,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,0CAA0C,CAAC;QACxE,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChE,MAAMT,OAAM,GAAI,MAAMM,QAAQ,CAACI,IAAI,CAAC,CAAC;;QAErC;QACA,IAAI,CAACC,eAAe,CAACX,OAAO,CAAC;QAC7B,IAAI,CAACC,OAAM,GAAI,KAAK;MACxB,EAAE,OAAOW,KAAK,EAAE;QACZ,IAAI,CAACV,YAAW,GAAI,6BAA6B;QACjDW,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClD,IAAI,CAACX,OAAM,GAAI,KAAK;MACxB;IACJ,CAAC;IAEDU,eAAeA,CAACX,O