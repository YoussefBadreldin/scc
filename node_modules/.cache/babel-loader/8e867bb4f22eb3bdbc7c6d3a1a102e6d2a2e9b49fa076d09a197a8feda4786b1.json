{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'JoinUnion',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      committees: [{\n        name: 'لجنة الأسر',\n        members: [1, 2, 3, 4, 5]\n      }, {\n        name: 'اللجنة الرياضية',\n        members: [6, 7, 8, 9, 10]\n      }\n      // Other committees...\n      ],\n      membersMap: {} // Storing all members by their title or ID\n    };\n  },\n  async mounted() {\n    window.scrollTo(0, 0);\n    await this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const members = await response.json();\n        this.membersMap = members.reduce((map, member) => {\n          map[member.member_title] = member; // Store members based on their title (e.g., رئيس الاتحاد, نائب رئيس الاتحاد)\n          return map;\n        }, {});\n      } catch (error) {\n        console.error('Error loading member data:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","committees","members","membersMap","mounted","window","scrollTo","loadMemberData","methods","response","fetch","ok","Error","json","reduce","map","member","member_title","error","console"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <!-- President -->\r\n                <div class=\"position president\" v-if=\"membersMap['رئيس الاتحاد']\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${membersMap['رئيس الاتحاد'].member_id}.jpg`\" alt=\"رئيس الاتحاد\" class=\"circle-photo\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ membersMap['رئيس الاتحاد'].member_name }}</div>\r\n                        <div class=\"title\">رئيس الاتحاد</div>\r\n                        <div class=\"faculty\">{{ membersMap['رئيس الاتحاد'].member_faculty }}</div>\r\n                        <div class=\"level\">المستوى {{ membersMap['رئيس الاتحاد'].member_level }}</div>\r\n                    </div>\r\n                </div>\r\n\r\n                <!-- Vice President -->\r\n                <div class=\"subordinates\" v-if=\"membersMap['نائب رئيس الاتحاد']\">\r\n                    <div class=\"position vice-president\">\r\n                        <div class=\"profile\">\r\n                            <img :src=\"`../../images/members/${membersMap['نائب رئيس الاتحاد'].member_id}.jpg`\" alt=\"نائب رئيس الاتحاد\" class=\"circle-photo\" />\r\n                        </div>\r\n                        <div class=\"details\">\r\n                            <div class=\"name\">{{ membersMap['نائب رئيس الاتحاد'].member_name }}</div>\r\n                            <div class=\"title\">نائب رئيس الاتحاد</div>\r\n                            <div class=\"faculty\">{{ membersMap['نائب رئيس الاتحاد'].member_faculty }}</div>\r\n                            <div class=\"level\">المستوى {{ membersMap['نائب رئيس الاتحاد'].member_level }}</div>\r\n                        </div>\r\n\r\n                        <!-- Committee Heads and Vice Heads -->\r\n                        <div class=\"subordinates\">\r\n                            <div class=\"position head\" v-for=\"(committee, index) in committees\" :key=\"index\">\r\n                                <div class=\"profile\" v-if=\"membersMap[`أمين ${committee.name}`]\">\r\n                                    <img :src=\"`../../images/members/${membersMap[`أمين ${committee.name}`].member_id}.jpg`\" :alt=\"`أمين ${committee.name}`\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\" v-if=\"membersMap[`أمين ${committee.name}`]\">\r\n                                    <div class=\"name\">{{ membersMap[`أمين ${committee.name}`].member_name }}</div>\r\n                                    <div class=\"title\">أمين {{ committee.name }}</div>\r\n                                    <div class=\"faculty\">{{ membersMap[`أمين ${committee.name}`].member_faculty }}</div>\r\n                                    <div class=\"level\">المستوى {{ membersMap[`أمين ${committee.name}`].member_level }}</div>\r\n                                </div>\r\n                                <div class=\"subordinates\" v-if=\"membersMap[`أمين مساعد ${committee.name}`]\">\r\n                                    <div class=\"position vice-head\">\r\n                                        <div class=\"profile\">\r\n                                            <img :src=\"`../../images/members/${membersMap[`أمين مساعد ${committee.name}`].member_id}.jpg`\" :alt=\"`أمين مساعد ${committee.name}`\" class=\"circle-photo\" />\r\n                                        </div>\r\n                                        <div class=\"details\">\r\n                                            <div class=\"name\">{{ membersMap[`أمين مساعد ${committee.name}`].member_name }}</div>\r\n                                            <div class=\"title\">أمين مساعد {{ committee.name }}</div>\r\n                                            <div class=\"faculty\">{{ membersMap[`أمين مساعد ${committee.name}`].member_faculty }}</div>\r\n                                            <div class=\"level\">المستوى {{ membersMap[`أمين مساعد ${committee.name}`].member_level }}</div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <h2 class=\"text-center my-4\">المكاتب التنفيذية للجان الأنشطة</h2>\r\n            <div class=\"executive-committee\">\r\n                <div class=\"committee-box\" v-for=\"(committee, index) in committees\" :key=\"index\">\r\n                    <div class=\"committee-title\">{{ committee.name }}</div>\r\n                    <div class=\"committee-content\">\r\n                        <!-- Committee Head -->\r\n                        <div class=\"position head\" v-if=\"membersMap[`أمين ${committee.name}`]\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${membersMap[`أمين ${committee.name}`].member_id}.jpg`\" :alt=\"`أمين ${committee.name}`\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ membersMap[`أمين ${committee.name}`].member_name }}</div>\r\n                                <div class=\"title\">أمين {{ committee.name }}</div>\r\n                                <div class=\"faculty\">{{ membersMap[`أمين ${committee.name}`].member_faculty }}</div>\r\n                                <div class=\"level\">المستوى {{ membersMap[`أمين ${committee.name}`].member_level }}</div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!-- Committee Vice Head -->\r\n                        <div class=\"position vice-head\" v-if=\"membersMap[`أمين مساعد ${committee.name}`]\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${membersMap[`أمين مساعد ${committee.name}`].member_id}.jpg`\" :alt=\"`أمين مساعد ${committee.name}`\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ membersMap[`أمين مساعد ${committee.name}`].member_name }}</div>\r\n                                <div class=\"title\">أمين مساعد {{ committee.name }}</div>\r\n                                <div class=\"faculty\">{{ membersMap[`أمين مساعد ${committee.name}`].member_faculty }}</div>\r\n                                <div class=\"level\">المستوى {{ membersMap[`أمين مساعد ${committee.name}`].member_level }}</div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!-- Committee Members -->\r\n                        <div class=\"members\">\r\n                            <div class=\"member\" v-for=\"memberId in committee.members\" :key=\"memberId\">\r\n                                <div class=\"profile\" v-if=\"membersMap[memberId]\">\r\n                                    <img :src=\"`../../images/members/${membersMap[memberId].member_id}.jpg`\" alt=\"عضو باللجنة\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\" v-if=\"membersMap[memberId]\">\r\n                                    <div class=\"name\">{{ membersMap[memberId].member_name }}</div>\r\n                                    <div class=\"title\">عضو باللجنة</div>\r\n                                    <div class=\"faculty\">{{ membersMap[memberId].member_faculty }}</div>\r\n                                    <div class=\"level\">المستوى {{ membersMap[memberId].member_level }}</div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\n\r\nexport default {\r\n    name: 'JoinUnion',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n    },\r\n    data() {\r\n        return {\r\n            committees: [\r\n                { name: 'لجنة الأسر', members: [1, 2, 3, 4, 5] },\r\n                { name: 'اللجنة الرياضية', members: [6, 7, 8, 9, 10] },\r\n                // Other committees...\r\n            ],\r\n            membersMap: {}, // Storing all members by their title or ID\r\n        };\r\n    },\r\n    async mounted() {\r\n        window.scrollTo(0, 0);\r\n        await this.loadMemberData();\r\n    },\r\n    methods: {\r\n        async loadMemberData() {\r\n            try {\r\n                const response = await fetch('https://aiusu-backend.vercel.app/members');\r\n                if (!response.ok) throw new Error('Failed to fetch member data');\r\n                const members = await response.json();\r\n                \r\n                this.membersMap = members.reduce((map, member) => {\r\n                    map[member.member_title] = member;  // Store members based on their title (e.g., رئيس الاتحاد, نائب رئيس الاتحاد)\r\n                    return map;\r\n                }, {});\r\n            } catch (error) {\r\n                console.error('Error loading member data:', error);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n    direction: rtl;\r\n}\r\n.organization-chart, .executive-committee {\r\n    margin: 20px 0;\r\n}\r\n.position {\r\n    text-align: center;\r\n}\r\n.profile {\r\n    margin: 10px;\r\n}\r\n.circle-photo {\r\n    border-radius: 50%;\r\n    width: 80px;\r\n    height: 80px;\r\n}\r\n.details {\r\n    padding: 5px;\r\n}\r\n.name, .title {\r\n    font-weight: bold;\r\n}\r\n.subordinates {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.members {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n}\r\n.member {\r\n    width: 60px;\r\n}\r\n</style>\r\n"],"mappings":"AAqHA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,CACR;QAAEH,IAAI,EAAE,YAAY;QAAEI,OAAO,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC;MAAE,CAAC,EAChD;QAAEJ,IAAI,EAAE,iBAAiB;QAAEI,OAAO,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE;MAAE;MACrD;MAAA,CACH;MACDC,UAAU,EAAE,CAAC,CAAC,CAAE;IACpB,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;IACrB,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;EAC/B,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,0CAA0C,CAAC;QACxE,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChE,MAAMV,OAAM,GAAI,MAAMO,QAAQ,CAACI,IAAI,CAAC,CAAC;QAErC,IAAI,CAACV,UAAS,GAAID,OAAO,CAACY,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;UAC9CD,GAAG,CAACC,MAAM,CAACC,YAAY,IAAID,MAAM,EAAG;UACpC,OAAOD,GAAG;QACd,CAAC,EAAE,CAAC,CAAC,CAAC;MACV,EAAE,OAAOG,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACtD;IACJ;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}