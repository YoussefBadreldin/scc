{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      president: {},\n      vicePresident: {},\n      heads: this.initializeHeads(),\n      viceHeads: this.initializeViceHeads(),\n      members: this.initializeMembers(),\n      loading: true,\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const memberData = await response.json();\n\n        // Organize members\n        this.organizeMembers(memberData);\n      } catch (error) {\n        this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\n        console.error('Error loading member data:', error);\n      } finally {\n        this.loading = false; // Ensure loading state is updated in both success and error cases\n      }\n    },\n    initializeHeads() {\n      const titles = ['Head_Scout', 'Head_Scientific', 'Head_Cultural', 'Head_Clubs', 'Head_Arts', 'Head_Sports', 'Head_Social'];\n      return Object.fromEntries(titles.map(title => [title, {}]));\n    },\n    initializeViceHeads() {\n      const titles = ['Vice_Scout', 'Vice_Scientific', 'Vice_Cultural', 'Vice_Clubs', 'Vice_Arts', 'Vice_Sports', 'Vice_Social'];\n      return Object.fromEntries(titles.map(title => [title, {}]));\n    },\n    initializeMembers() {\n      const titles = ['Member_Scout', 'Member_Scientific', 'Member_Cultural', 'Member_Clubs', 'Member_Arts', 'Member_Sports', 'Member_Social'];\n      return Object.fromEntries(titles.map(title => [title, []]));\n    },\n    organizeMembers(members) {\n      // Extract president and vice president\n      this.president = members.find(m => m.member_title === 'President') || {};\n      this.vicePresident = members.find(m => m.member_title === 'Vice') || {};\n\n      // Organize heads and vice heads\n      for (const title of Object.keys(this.heads)) {\n        this.heads[title] = members.find(m => m.member_title === title) || {};\n      }\n      for (const title of Object.keys(this.viceHeads)) {\n        this.viceHeads[title] = members.find(m => m.member_title === title) || {};\n      }\n\n      // Organize members by title\n      for (const title of Object.keys(this.members)) {\n        this.members[title] = members.filter(m => m.member_title === title) || [];\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","president","vicePresident","heads","initializeHeads","viceHeads","initializeViceHeads","members","initializeMembers","loading","errorMessage","mounted","loadMemberData","methods","response","fetch","ok","Error","memberData","json","organizeMembers","error","console","titles","Object","fromEntries","map","title","find","m","member_title","keys","filter"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${president.member_id}.jpg`\" :alt=\"president.member_name\" class=\"circle-photo\" v-if=\"president.member_id\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ president.member_name || 'غير متوفر' }}</div>\r\n                        <div class=\"title\">رئيس الاتحاد</div>\r\n              