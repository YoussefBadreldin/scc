{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'OrganizationalChart',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      organization: {\n        president: {\n          name: 'John Doe',\n          photo: '../../images/president.jpg',\n          faculty: 'Engineering',\n          level: 'Senior'\n        },\n        vicePresident: {\n          name: 'Jane Smith',\n          photo: '../../images/vice-president.jpg',\n          faculty: 'Business',\n          level: 'Junior'\n        },\n        committees: [{\n          name: 'Committee 1',\n          head: {\n            name: 'Head 1',\n            photo: '../../images/head1.jpg',\n            faculty: 'Law',\n            level: 'Senior'\n          },\n          viceHead: {\n            name: 'Vice Head 1',\n            photo: '../../images/vice-head1.jpg',\n            faculty: 'Law',\n            level: 'Junior'\n          }\n        }, {\n          name: 'Committee 2',\n          head: {\n            name: 'Head 2',\n            photo: '../../images/head2.jpg',\n            faculty: 'Medicine',\n            level: 'Senior'\n          },\n          viceHead: {\n            name: 'Vice Head 2',\n            photo: '../../images/vice-head2.jpg',\n            faculty: 'Medicine',\n            level: 'Junior'\n          }\n        }\n        // Repeat for the rest of the committees...\n        ]\n      }\n    };\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","organization","president","photo","faculty","level","vicePresident","committees","head","viceHead"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">Organizational Chart</h2>\r\n\r\n            <div class=\"org-chart\">\r\n                <div class=\"level president\">\r\n                    <h3>President</h3>\r\n                    <div class=\"person\">\r\n                        <img :src=\"organization.president.photo\" alt=\"President's Photo\" class=\"photo\" />\r\n                        <div class=\"details\">\r\n                            <p><strong>Name:</strong> {{ organization.president.name }}</p>\r\n                            <p><strong>Title:</strong> President</p>\r\n                            <p><strong>Faculty:</strong> {{ organization.president.faculty }}</p>\r\n                            <p><strong>Level:</strong> {{ organization.president.level }}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"level vice-president\">\r\n                    <h4>Vice President</h4>\r\n                    <div class=\"person\">\r\n                        <img :src=\"organization.vicePresident.photo\" alt=\"Vice President's Photo\" class=\"photo\" />\r\n                        <div class=\"details\">\r\n                            <p><strong>Name:</strong> {{ organization.vicePresident.name }}</p>\r\n                            <p><strong>Title:</strong> Vice President</p>\r\n                            <p><strong>Faculty:</strong> {{ organization.vicePresident.faculty }}</p>\r\n                            <p><strong>Level:</strong> {{ organization.vicePresident.level }}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"committees\">\r\n                    <div class=\"committee\" v-for=\"(committee, index) in organization.committees\" :key=\"index\">\r\n                        <h5>{{ committee.name }}</h5>\r\n\r\n                        <div class=\"person\">\r\n                            <img :src=\"committee.head.photo\" alt=\"Head's Photo\" class=\"photo\" />\r\n                            <div class=\"details\">\r\n                                <p><strong>Name:</strong> {{ committee.head.name }}</p>\r\n                                <p><strong>Title:</strong> Head</p>\r\n                                <p><strong>Faculty:</strong> {{ committee.head.faculty }}</p>\r\n                                <p><strong>Level:</strong> {{ committee.head.level }}</p>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class=\"person\">\r\n                            <img :src=\"committee.viceHead.photo\" alt=\"Vice Head's Photo\" class=\"photo\" />\r\n                            <div class=\"details\">\r\n                                <p><strong>Name:</strong> {{ committee.viceHead.name }}</p>\r\n                                <p><strong>Title:</strong> Vice Head</p>\r\n                                <p><strong>Faculty:</strong> {{ committee.viceHead.faculty }}</p>\r\n                                <p><strong>Level:</strong> {{ committee.viceHead.level }}</p>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\n\r\nexport default {\r\n    name: 'OrganizationalChart',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n    },\r\n    data() {\r\n        return {\r\n            organization: {\r\n                president: {\r\n                    name: 'John Doe',\r\n                    photo: '../../images/president.jpg',\r\n                    faculty: 'Engineering',\r\n                    level: 'Senior',\r\n                },\r\n                vicePresident: {\r\n                    name: 'Jane Smith',\r\n                    photo: '../../images/vice-president.jpg',\r\n                    faculty: 'Business',\r\n                    level: 'Junior',\r\n                },\r\n                committees: [\r\n                    {\r\n                        name: 'Committee 1',\r\n                        head: {\r\n                            name: 'Head 1',\r\n                            photo: '../../images/head1.jpg',\r\n                            faculty: 'Law',\r\n                            level: 'Senior',\r\n                        },\r\n                        viceHead: {\r\n                            name: 'Vice Head 1',\r\n                            photo: '../../images/vice-head1.jpg',\r\n                            faculty: 'Law',\r\n                            level: 'Junior',\r\n                        },\r\n                    },\r\n                    {\r\n                        name: 'Committee 2',\r\n                        head: {\r\n                            name: 'Head 2',\r\n                            photo: '../../images/head2.jpg',\r\n                            faculty: 'Medicine',\r\n                            level: 'Senior',\r\n                        },\r\n                        viceHead: {\r\n                            name: 'Vice Head 2',\r\n                            photo: '../../images/vice-head2.jpg',\r\n                            faculty: 'Medicine',\r\n                            level: 'Junior',\r\n                        },\r\n                    },\r\n                    // Repeat for the rest of the committees...\r\n                ],\r\n            },\r\n        };\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n    text-align: center;\r\n}\r\n\r\n.org-chart {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n}\r\n\r\n.level {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.committees {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.committee {\r\n    border: 1px solid #ddd;\r\n    padding: 15px;\r\n    margin: 10px;\r\n    border-radius: 8px;\r\n    width: 30%;\r\n    box-sizing: border-box;\r\n}\r\n\r\n.person {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.photo {\r\n    width: 70px;\r\n    height: 70px;\r\n    border-radius: 50%;\r\n    margin-right: 10px;\r\n}\r\n\r\n.details {\r\n    text-align: left;\r\n}\r\n\r\nh3, h4, h5 {\r\n    margin-bottom: 10px;\r\n}\r\n\r\np {\r\n    margin: 3px 0;\r\n}\r\n</style>\r\n"],"mappings":"AAiEA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,eAAe;EACXC,IAAI,EAAE,qBAAqB;EAC3BC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE;QACVC,SAAS,EAAE;UACPJ,IAAI,EAAE,UAAU;UAChBK,KAAK,EAAE,4BAA4B;UACnCC,OAAO,EAAE,aAAa;UACtBC,KAAK,EAAE;QACX,CAAC;QACDC,aAAa,EAAE;UACXR,IAAI,EAAE,YAAY;UAClBK,KAAK,EAAE,iCAAiC;UACxCC,OAAO,EAAE,UAAU;UACnBC,KAAK,EAAE;QACX,CAAC;QACDE,UAAU,EAAE,CACR;UACIT,IAAI,EAAE,aAAa;UACnBU,IAAI,EAAE;YACFV,IAAI,EAAE,QAAQ;YACdK,KAAK,EAAE,wBAAwB;YAC/BC,OAAO,EAAE,KAAK;YACdC,KAAK,EAAE;UACX,CAAC;UACDI,QAAQ,EAAE;YACNX,IAAI,EAAE,aAAa;YACnBK,KAAK,EAAE,6BAA6B;YACpCC,OAAO,EAAE,KAAK;YACdC,KAAK,EAAE;UACX;QACJ,CAAC,EACD;UACIP,IAAI,EAAE,aAAa;UACnBU,IAAI,EAAE;YACFV,IAAI,EAAE,QAAQ;YACdK,KAAK,EAAE,wBAAwB;YAC/BC,OAAO,EAAE,UAAU;YACnBC,KAAK,EAAE;UACX,CAAC;UACDI,QAAQ,EAAE;YACNX,IAAI,EAAE,aAAa;YACnBK,KAAK,EAAE,6BAA6B;YACpCC,OAAO,EAAE,UAAU;YACnBC,KAAK,EAAE;UACX;QACJ;QACA;QAAA;MAER;IACJ,CAAC;EACL;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}