{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      president: {},\n      vicePresident: {},\n      heads: {},\n      viceHeads: {},\n      members: {\n        Member_Scout: [],\n        Member_Scientific: [],\n        Member_Cultural: [],\n        Member_Clubs: [],\n        Member_Arts: [],\n        Member_Sports: [],\n        Member_Social: []\n      },\n      loading: true,\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const memberData = await response.json();\n\n        // Organize members\n        this.organizeMembers(memberData);\n        this.loading = false;\n      } catch (error) {\n        this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\n        console.error('Error loading member data:', error);\n        this.loading = false;\n      }\n    },\n    organizeMembers(members) {\n      // Initialize default values\n      this.president = {};\n      this.vicePresident = {};\n      this.heads = {};\n      this.viceHeads = {};\n      this.members = {\n        Member_Scout: [],\n        Member_Scientific: [],\n        Member_Cultural: [],\n        Member_Clubs: [],\n        Member_Arts: [],\n        Member_Sports: [],\n        Member_Social: []\n      };\n\n      // Create a lookup for titles\n      const titleMap = {\n        'President': 'president',\n        'Vice': 'vicePresident',\n        'Head_Scout': 'heads',\n        'Head_Scientific': 'heads',\n        'Head_Cultural': 'heads',\n        'Head_Clubs': 'heads',\n        'Head_Arts': 'heads',\n        'Head_Sports': 'heads',\n        'Head_Social': 'heads',\n        'Vice_Scout': 'viceHeads',\n        'Vice_Scientific': 'viceHeads',\n        'Vice_Cultural': 'viceHeads',\n        'Vice_Clubs': 'viceHeads',\n        'Vice_Arts': 'viceHeads',\n        'Vice_Sports': 'viceHeads',\n        'Vice_Social': 'viceHeads',\n        'Member_Scout': 'members',\n        'Member_Scientific': 'members',\n        'Member_Cultural': 'members',\n        'Member_Clubs': 'members',\n        'Member_Arts': 'members',\n        'Member_Sports': 'members',\n        'Member_Social': 'members'\n      };\n\n      // Iterate through members once\n      members.forEach(member => {\n        const {\n          member_title\n        } = member;\n        switch (member_title) {\n          case 'President':\n            this.president = member;\n            break;\n          case 'Vice':\n            this.vicePresident = member;\n            break;\n          default:\n            if (titleMap[member_title]) {\n              if (member_title.startsWith('Head_') || member_title.startsWith('Vice_')) {\n                this[titleMap[member_title]][member_title] = member;\n              } else {\n                this.members[member_title].push(member);\n              }\n            }\n        }\n      });\n    },\n    formatMemberName(name) {\n      if (!name) return '';\n      const words = name.split(' ');\n      if (words.length === 1) return words[0]; // Return if there's only one word\n      return `${words[0]} ${words[words.length - 1]}`; // Return first and last words\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","president","vicePresident","heads","viceHeads","members","Member_Scout","Member_Scientific","Member_Cultural","Member_Clubs","Member_Arts","Member_Sports","Member_Social","loading","errorMessage","mounted","loadMemberData","methods","response","fetch","ok","Error","memberData","json","organizeMembers","error","console","titleMap","forEach","member","member_title","startsWith","push","formatMemberName","words","split","length"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">أعضاء الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${president.member_id}.jpg`\" :alt=\"president.member_name\" class=\"circle-photo\" v-if=\"president.member_id\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ formatmemberName(president.member_name) || 'جاري التحميل..' }}</div>\r\n                        <div class=\"title\">رئيس الاتحاد</div>\r\n                        <div class=\"faculty\">{{ president.member_faculty || 'جاري التحميل..' }}</div>\r\n                        <div class=\"level\">{{ president.member_level || 'جاري التحميل..' }}</div>\r\n                    </div>\r\n                    <div class=\"subordinates\">\r\n                        <div class=\"position vice-president\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${vicePresident.member_id}.jpg`\" :alt=\"vicePresident.member_name\" class=\"circle-photo\" v-if=\"vicePresident.member_id\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ formatmemberName(vicePresident.member_name) || 'جاري التحميل..' }}</div>\r\n                                <div class=\"title\">نائب رئيس الاتحاد</div>\r\n                                <div class=\"faculty\">{{ vic