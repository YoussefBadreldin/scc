{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nimport MemberPosition from './MemberPosition.vue';\nimport CommitteeBox from './CommitteeBox.vue';\nexport default {\n  name: 'JoinUnion',\n  components: {\n    HeaderComponent,\n    FooterComponent,\n    MemberPosition,\n    CommitteeBox\n  },\n  data() {\n    return {\n      committees: [{\n        name: 'لجنة الأسر',\n        headId: 1,\n        viceHeadId: 2,\n        members: [1, 2, 3, 4, 5]\n      }, {\n        name: 'اللجنة الرياضية',\n        headId: 6,\n        viceHeadId: 7,\n        members: [6, 7, 8, 9, 10]\n      }\n      // Other committees here...\n      ],\n      membersMap: {}\n    };\n  },\n  async mounted() {\n    window.scrollTo(0, 0);\n    await this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const members = await response.json();\n        this.membersMap = members.reduce((map, member) => {\n          map[member.member_id] = member;\n          return map;\n        }, {});\n      } catch (error) {\n        console.error('Error loading member data:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","MemberPosition","CommitteeBox","name","components","data","committees","headId","viceHeadId","members","membersMap","mounted","window","scrollTo","loadMemberData","methods","response","fetch","ok","Error","json","reduce","map","member","member_id","error","console"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <MemberPosition\r\n                    :member=\"membersMap.president\"\r\n                    title=\"رئيس الاتحاد\"\r\n                />\r\n                <div class=\"subordinates\">\r\n                    <MemberPosition\r\n                        :member=\"membersMap.vicePresident\"\r\n                        title=\"نائب رئيس الاتحاد\"\r\n                    />\r\n                    <div class=\"subordinates\">\r\n                        <div class=\"position head\" v-for=\"(committee, index) in committees\" :key=\"index\">\r\n                            <MemberPosition\r\n                                :member=\"membersMap[committee.headId]\"\r\n                                :title=\"`أمين ${committee.name}`\"\r\n                            />\r\n                            <div class=\"subordinates\">\r\n                                <MemberPosition\r\n                                    :member=\"membersMap[committee.viceHeadId]\"\r\n                                    :title=\"`أمين مساعد ${committee.name}`\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <h2 class=\"text-center my-4\">المكاتب التنفيذية للجان الأنشطة</h2>\r\n            <div class=\"executive-committee\">\r\n                <CommitteeBox v-for=\"(committee, index) in committees\" :key=\"index\" :committee=\"committee\" />\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\nimport MemberPosition from './MemberPosition.vue';\r\nimport CommitteeBox from './CommitteeBox.vue';\r\n\r\nexport default {\r\n    name: 'JoinUnion',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n        MemberPosition,\r\n        CommitteeBox,\r\n    },\r\n    data() {\r\n        return {\r\n            committees: [\r\n                {\r\n                    name: 'لجنة الأسر',\r\n                    headId: 1,\r\n                    viceHeadId: 2,\r\n                    members: [1, 2, 3, 4, 5],\r\n                },\r\n                {\r\n                    name: 'اللجنة الرياضية',\r\n                    headId: 6,\r\n                    viceHeadId: 7,\r\n                    members: [6, 7, 8, 9, 10],\r\n                },\r\n                // Other committees here...\r\n            ],\r\n            membersMap: {},\r\n        };\r\n    },\r\n    async mounted() {\r\n        window.scrollTo(0, 0);\r\n        await this.loadMemberData();\r\n    },\r\n    methods: {\r\n        async loadMemberData() {\r\n            try {\r\n                const response = await fetch('https://aiusu-backend.vercel.app/members');\r\n                if (!response.ok) throw new Error('Failed to fetch member data');\r\n                const members = await response.json();\r\n                \r\n                this.membersMap = members.reduce((map, member) => {\r\n                    map[member.member_id] = member;\r\n                    return map;\r\n                }, {});\r\n            } catch (error) {\r\n                console.error('Error loading member data:', error);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n    direction: rtl;\r\n}\r\n.organization-chart {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin: 20px 0;\r\n}\r\n.subordinates {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.executive-committee {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-between;\r\n    gap: 10px;\r\n}\r\n@media (max-width: 768px) {\r\n    .committee-box {\r\n        width: calc(50% - 20px);\r\n    }\r\n}\r\n@media (max-width: 576px) {\r\n    .committee-box {\r\n        width: 100%;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AA0CA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AACrE,OAAOC,cAAa,MAAO,sBAAsB;AACjD,OAAOC,YAAW,MAAO,oBAAoB;AAE7C,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACRL,eAAe;IACfC,eAAe;IACfC,cAAc;IACdC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,CACR;QACIH,IAAI,EAAE,YAAY;QAClBI,MAAM,EAAE,CAAC;QACTC,UAAU,EAAE,CAAC;QACbC,OAAO,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC;MAC3B,CAAC,EACD;QACIN,IAAI,EAAE,iBAAiB;QACvBI,MAAM,EAAE,CAAC;QACTC,UAAU,EAAE,CAAC;QACbC,OAAO,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE;MAC5B;MACA;MAAA,CACH;MACDC,UAAU,EAAE,CAAC;IACjB,CAAC;EACL,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;IACrB,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;EAC/B,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,0CAA0C,CAAC;QACxE,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChE,MAAMV,OAAM,GAAI,MAAMO,QAAQ,CAACI,IAAI,CAAC,CAAC;QAErC,IAAI,CAACV,UAAS,GAAID,OAAO,CAACY,MAAM,CAAC,CAACC,GAAG,EAAEC,MAAM,KAAK;UAC9CD,GAAG,CAACC,MAAM,CAACC,SAAS,IAAID,MAAM;UAC9B,OAAOD,GAAG;QACd,CAAC,EAAE,CAAC,CAAC,CAAC;MACV,EAAE,OAAOG,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACtD;IACJ;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}