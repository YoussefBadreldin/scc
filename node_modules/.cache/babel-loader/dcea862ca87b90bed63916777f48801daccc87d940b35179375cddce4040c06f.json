{"ast":null,"code":"import HeaderComponent from '../../public/global/headerComponent.vue';\nimport FooterComponent from '../../public/global/footerComponent.vue';\nexport default {\n  name: 'FindCommittee',\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      president: {},\n      vicePresident: {},\n      committeeHeads: [],\n      // Array to hold committee heads\n      committees: [],\n      // Array to hold all committees with their members\n      loading: true,\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.loadMemberData();\n  },\n  methods: {\n    async loadMemberData() {\n      try {\n        const response = await fetch('https://aiusu-backend.vercel.app/members');\n        if (!response.ok) throw new Error('Failed to fetch member data');\n        const members = await response.json();\n\n        // Organize members\n        this.organizeMembers(members);\n        this.loading = false;\n      } catch (error) {\n        this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\n        console.error('Error loading member data:', error);\n        this.loading = false;\n      }\n    },\n    organizeMembers(members) {\n      // Extract president and vice president\n      this.president = members.find(m => m.member_title === 'President');\n      this.vicePresident = members.find(m => m.member_title === 'Vice');\n\n      // Organize committee heads\n      this.committeeHeads = members.filter(m => m.member_title.startsWith('Head_')).map(head => ({\n        head,\n        title: head.member_title.replace(/_/g, ' ').substring(5),\n        // Extract title\n        faculty: head.member_faculty,\n        level: head.member_level,\n        viceHead: members.find(m => m.member_title === `Vice_${head.member_title.split('_')[1]}`),\n        // Find vice head\n        members: members.filter(m => m.member_title.startsWith(head.member_title.replace(/_/g, ' '))) // Find committee members\n      }));\n\n      // Organize all committees\n      this.committees = members.filter(m => m.member_title.startsWith('Member_')).map(member => ({\n        title: member.member_title.replace(/_/g, ' ').substring(7),\n        // Extract title\n        faculty: member.member_faculty,\n        level: member.member_level,\n        head: this.committeeHeads.find(h => h.head.member_title === `Head_${member.member_title.split('_')[1]}`),\n        // Find head\n        members: members.filter(m => m.member_title.startsWith(member.member_title.replace(/_/g, ' '))) // Find committee members\n      }));\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","name","components","data","president","vicePresident","committeeHeads","committees","loading","errorMessage","mounted","loadMemberData","methods","response","fetch","ok","Error","members","json","organizeMembers","error","console","find","m","member_title","filter","startsWith","map","head","title","replace","substring","faculty","member_faculty","level","member_level","viceHead","split","member","h"],"sources":["C:\\Users\\Youssef Badreldin\\Documents\\GitHub\\AIUSU\\src\\views\\members.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent />\r\n        <div class=\"container\">\r\n            <h2 class=\"text-center my-4\">مجلس الاتحاد</h2>\r\n            <div class=\"organization-chart\">\r\n                <div class=\"position president\">\r\n                    <div class=\"profile\">\r\n                        <img :src=\"`../../images/members/${president.member_id}.jpg`\" :alt=\"president.member_name\" class=\"circle-photo\" />\r\n                    </div>\r\n                    <div class=\"details\">\r\n                        <div class=\"name\">{{ president.member_name }}</div>\r\n                        <div class=\"title\">{{ president.member_title.replace(/_/g, ' ') }}</div>\r\n                        <div class=\"faculty\">{{ president.member_faculty }}</div>\r\n                        <div class=\"level\">{{ president.member_level }}</div>\r\n                    </div>\r\n                    <div class=\"subordinates\">\r\n                        <div class=\"position vice-president\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${vicePresident.member_id}.jpg`\" :alt=\"vicePresident.member_name\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ vicePresident.member_name }}</div>\r\n                                <div class=\"title\">{{ vicePresident.member_title.replace(/_/g, ' ') }}</div>\r\n                                <div class=\"faculty\">{{ vicePresident.member_faculty }}</div>\r\n                                <div class=\"level\">{{ vicePresident.member_level }}</div>\r\n                            </div>\r\n                            <div class=\"subordinates\">\r\n                                <div class=\"position head\" v-for=\"(committee, index) in committeeHeads\" :key=\"index\">\r\n                                    <div class=\"profile\">\r\n                                        <img :src=\"`../../images/members/${committee.head.member_id}.jpg`\" :alt=\"committee.head.member_name\" class=\"circle-photo\" />\r\n                                    </div>\r\n                                    <div class=\"details\">\r\n                                        <div class=\"name\">{{ committee.head.member_name }}</div>\r\n                                        <div class=\"title\">{{ committee.head.member_title.replace(/_/g, ' ') }}</div>\r\n                                        <div class=\"faculty\">{{ committee.head.member_faculty }}</div>\r\n                                        <div class=\"level\">{{ committee.head.member_level }}</div>\r\n                                    </div>\r\n                                    <div class=\"subordinates\">\r\n                                        <div class=\"position vice-head\">\r\n                                            <div class=\"profile\">\r\n                                                <img :src=\"`../../images/members/${committee.viceHead.member_id}.jpg`\" :alt=\"committee.viceHead.member_name\" class=\"circle-photo\" />\r\n                                            </div>\r\n                                            <div class=\"details\">\r\n                                                <div class=\"name\">{{ committee.viceHead.member_name }}</div>\r\n                                                <div class=\"title\">{{ committee.viceHead.member_title.replace(/_/g, ' ') }}</div>\r\n                                                <div class=\"faculty\">{{ committee.viceHead.member_faculty }}</div>\r\n                                                <div class=\"level\">{{ committee.viceHead.member_level }}</div>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <h2 class=\"text-center my-4\">المكاتب التنفيذية للجان الأنشطة</h2>\r\n            <div class=\"executive-committee\">\r\n                <div class=\"committee-box\" v-for=\"(committee, index) in committees\" :key=\"index\">\r\n                    <div class=\"committee-title\">{{ committee.title }}</div>\r\n                    <div class=\"committee-content\">\r\n                        <div class=\"position head\">\r\n                            <div class=\"profile\">\r\n                                <img :src=\"`../../images/members/${committee.head.member_id}.jpg`\" :alt=\"committee.head.member_name\" class=\"circle-photo\" />\r\n                            </div>\r\n                            <div class=\"details\">\r\n                                <div class=\"name\">{{ committee.head.member_name }}</div>\r\n                                <div class=\"title\">{{ committee.head.member_title.replace(/_/g, ' ') }}</div>\r\n                                <div class=\"faculty\">{{ committee.head.member_faculty }}</div>\r\n                                <div class=\"level\">{{ committee.head.member_level }}</div>\r\n                            </div>\r\n                            <div class=\"subordinates\">\r\n                                <div class=\"position vice-head\">\r\n                                    <div class=\"profile\">\r\n                                        <img :src=\"`../../images/members/${committee.viceHead.member_id}.jpg`\" :alt=\"committee.viceHead.member_name\" class=\"circle-photo\" />\r\n                                    </div>\r\n                                    <div class=\"details\">\r\n                                        <div class=\"name\">{{ committee.viceHead.member_name }}</div>\r\n                                        <div class=\"title\">{{ committee.viceHead.member_title.replace(/_/g, ' ') }}</div>\r\n                                        <div class=\"faculty\">{{ committee.viceHead.member_faculty }}</div>\r\n                                        <div class=\"level\">{{ committee.viceHead.member_level }}</div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"members\">\r\n                            <div class=\"member\" v-for=\"member in committee.members\" :key=\"member.member_id\">\r\n                                <div class=\"profile\">\r\n                                    <img :src=\"`../../images/members/${member.member_id}.jpg`\" :alt=\"member.member_name\" class=\"circle-photo\" />\r\n                                </div>\r\n                                <div class=\"details\">\r\n                                    <div class=\"name\">{{ member.member_name }}</div>\r\n                                    <div class=\"title\">عضو باللجنة</div>\r\n                                    <div class=\"faculty\">{{ committee.faculty }}</div>\r\n                                    <div class=\"level\">{{ committee.level }}</div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <FooterComponent />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from '../../public/global/headerComponent.vue';\r\nimport FooterComponent from '../../public/global/footerComponent.vue';\r\n\r\nexport default {\r\n    name: 'FindCommittee',\r\n    components: {\r\n        HeaderComponent,\r\n        FooterComponent,\r\n    },\r\n    data() {\r\n        return {\r\n            president: {},\r\n            vicePresident: {},\r\n            committeeHeads: [], // Array to hold committee heads\r\n            committees: [], // Array to hold all committees with their members\r\n            loading: true,\r\n            errorMessage: '',\r\n        };\r\n    },\r\n    mounted() {\r\n        this.loadMemberData();\r\n    },\r\n    methods: {\r\n        async loadMemberData() {\r\n            try {\r\n                const response = await fetch('https://aiusu-backend.vercel.app/members');\r\n                if (!response.ok) throw new Error('Failed to fetch member data');\r\n                const members = await response.json();\r\n\r\n                // Organize members\r\n                this.organizeMembers(members);\r\n                this.loading = false;\r\n            } catch (error) {\r\n                this.errorMessage = 'خطأ في تحميل بيانات الأعضاء';\r\n                console.error('Error loading member data:', error);\r\n                this.loading = false;\r\n            }\r\n        },\r\n\r\n        organizeMembers(members) {\r\n            // Extract president and vice president\r\n            this.president = members.find(m => m.member_title === 'President');\r\n            this.vicePresident = members.find(m => m.member_title === 'Vice');\r\n\r\n            // Organize committee heads\r\n            this.committeeHeads = members.filter(m => \r\n                m.member_title.startsWith('Head_')\r\n            ).map(head => ({\r\n                head,\r\n                title: head.member_title.replace(/_/g, ' ').substring(5), // Extract title\r\n                faculty: head.member_faculty,\r\n                level: head.member_level,\r\n                viceHead: members.find(m => m.member_title === `Vice_${head.member_title.split('_')[1]}`), // Find vice head\r\n                members: members.filter(m => m.member_title.startsWith(head.member_title.replace(/_/g, ' '))) // Find committee members\r\n            }));\r\n\r\n            // Organize all committees\r\n            this.committees = members.filter(m =>\r\n                m.member_title.startsWith('Member_')\r\n            ).map(member => ({\r\n                title: member.member_title.replace(/_/g, ' ').substring(7), // Extract title\r\n                faculty: member.member_faculty,\r\n                level: member.member_level,\r\n                head: this.committeeHeads.find(h => h.head.member_title === `Head_${member.member_title.split('_')[1]}`), // Find head\r\n                members: members.filter(m => m.member_title.startsWith(member.member_title.replace(/_/g, ' '))) // Find committee members\r\n            }));\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n    padding: 20px;\r\n    direction: rtl; /* Set the text direction to RTL */\r\n}\r\n.organization-chart {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin: 20px 0;\r\n}\r\n.position {\r\n    margin: 10px 0;\r\n    text-align: center;\r\n}\r\n.profile {\r\n    margin: 10px;\r\n}\r\n.circle-photo {\r\n    border-radius: 50%;\r\n    width: 80px; /* Adjust size as necessary */\r\n    height: 80px; /* Ensure it’s a circle */\r\n}\r\n.details {\r\n    padding: 5px;\r\n}\r\n.name {\r\n    font-weight: bold;\r\n}\r\n.title {\r\n    font-weight: bold;\r\n}\r\n.subordinates {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.executive-committee {\r\n    display: flex;\r\n    flex-wrap: wrap; /* Allow wrapping to create multiple rows */\r\n    justify-content: space-between; /* Distribute space between committee boxes */\r\n    gap: 10px; /* Optional: Add some spacing between boxes */\r\n}\r\n.committee-box {\r\n    border: 1px solid #dee2e6;\r\n    border-radius: 5px;\r\n    padding: 10px;\r\n    width: calc(33.33% - 20px); /* 3 boxes per row minus margin */\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n    box-sizing: border-box; /* Include padding and border in element's total width and height */\r\n}\r\n.committee-title {\r\n    font-weight: bold;\r\n    font-size: 1.2rem;\r\n    text-align: center;\r\n    margin-bottom: 10px;\r\n}\r\n.committee-content {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n.members {\r\n    display: flex;\r\n    flex-wrap: wrap; /* Allow members to wrap to the next line */\r\n    justify-content: space-around; /* Center members with space around */\r\n}\r\n\r\n.member {\r\n    width: 25%; /* 4 members per row */\r\n    margin-bottom: 10px; /* Add some space between rows */\r\n    box-sizing: border-box; /* Include padding and borders in width */\r\n    text-align: center; /* Center member details */\r\n}\r\n@media (max-width: 768px) {\r\n    .committee-box {\r\n        width: calc(50% - 20px); /* 2 boxes per row on tablets */\r\n    }\r\n}\r\n@media (max-width: 576px) {\r\n    .committee-box {\r\n        width: 100%; /* 1 box per row on mobile */\r\n    }\r\n    .title {\r\n        font-size: 0.8rem; /* Reduce font size on small screens */\r\n    }\r\n    .committee-box {\r\n        width: 100%; /* 1 box per row on mobile */\r\n    }\r\n    .title, .name, .committee-title {\r\n        font-size: 0.45rem; /* Reduce font size on small screens */\r\n    }\r\n    \r\n    h2 {\r\n        font-size: 1.8rem; /* Reduce heading size on mobile */\r\n    }\r\n    .details {\r\n        font-size: 0.45rem; /* Reduce overall details font size */\r\n    }\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA6GA,OAAOA,eAAc,MAAO,yCAAyC;AACrE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,CAAC,CAAC;MACbC,aAAa,EAAE,CAAC,CAAC;MACjBC,cAAc,EAAE,EAAE;MAAE;MACpBC,UAAU,EAAE,EAAE;MAAE;MAChBC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,cAAc,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,cAAcA,CAAA,EAAG;MACnB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,0CAA0C,CAAC;QACxE,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChE,MAAMC,OAAM,GAAI,MAAMJ,QAAQ,CAACK,IAAI,CAAC,CAAC;;QAErC;QACA,IAAI,CAACC,eAAe,CAACF,OAAO,CAAC;QAC7B,IAAI,CAACT,OAAM,GAAI,KAAK;MACxB,EAAE,OAAOY,KAAK,EAAE;QACZ,IAAI,CAACX,YAAW,GAAI,6BAA6B;QACjDY,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClD,IAAI,CAACZ,OAAM,GAAI,KAAK;MACxB;IACJ,CAAC;IAEDW,eAAeA,CAACF,OAAO,EAAE;MACrB;MACA,IAAI,CAACb,SAAQ,GAAIa,OAAO,CAACK,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,YAAW,KAAM,WAAW,CAAC;MAClE,IAAI,CAACnB,aAAY,GAAIY,OAAO,CAACK,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,YAAW,KAAM,MAAM,CAAC;;MAEjE;MACA,IAAI,CAAClB,cAAa,GAAIW,OAAO,CAACQ,MAAM,CAACF,CAAA,IACjCA,CAAC,CAACC,YAAY,CAACE,UAAU,CAAC,OAAO,CACrC,CAAC,CAACC,GAAG,CAACC,IAAG,KAAM;QACXA,IAAI;QACJC,KAAK,EAAED,IAAI,CAACJ,YAAY,CAACM,OAAO,CAAC,IAAI,EAAE,GAAG,CAAC,CAACC,SAAS,CAAC,CAAC,CAAC;QAAE;QAC1DC,OAAO,EAAEJ,IAAI,CAACK,cAAc;QAC5BC,KAAK,EAAEN,IAAI,CAACO,YAAY;QACxBC,QAAQ,EAAEnB,OAAO,CAACK,IAAI,